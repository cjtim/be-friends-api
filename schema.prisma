datasource db {
    // could be postgresql or mysql
    provider = "sqlite"
    url      = "file:dev.db"
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
    // set the output folder and package name
    output           = "./repository"
    package          = "repository"
}

model User {
    id        String   @default(cuid()) @id
    name      String
    loginMethodId Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

